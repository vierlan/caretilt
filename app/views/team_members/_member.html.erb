<tr id="<%= dom_id(user) %>" class="border-t border-gray-300">
  <td class="whitespace-nowrap py-4 pl-4 pr-3 text-base font-medium text-gray-900 sm:pl-3"><%= user.first_name %></td>
  <td class="whitespace-nowrap px-3 py-4 text-base text-gray-500"><%= user.last_name %></td>
  <td class="whitespace-nowrap px-3 py-4 text-base text-gray-500"><%= user.email %></td>
  <td class="whitespace-nowrap px-3 py-4 text-base text-gray-500"><%= user.role %></td>
    <td class="whitespace-nowrap px-3 py-4 text-base text-gray-500"><%= user.status %></td>

  <td class="relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-base font-medium sm:pr-3">
    <a href="#" class="text-primary hover:text-indigo-900">Edit<span class="sr-only">, <%= user.email %></span></a>
  </td>
</tr>
